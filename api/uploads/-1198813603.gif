<div id="answer-9210901" class="answer accepted-answer" data-answerid="9210901"  itemscope itemtype="http://schema.org/Answer" itemprop="acceptedAnswer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
    <input type="hidden" name="_id_" value="9210901">
    <a class="vote-up-off" title="This answer is useful">up vote</a>
    <span itemprop="upvoteCount" class="vote-count-post ">33</span>
    <a class="vote-down-off" title="This answer is not useful">down vote</a>
    


